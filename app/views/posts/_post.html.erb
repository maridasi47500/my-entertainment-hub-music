<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= post.author %>
  </p>

  <p>
    <strong>Pictext:</strong>
    <%= post.pictext %>
  </p>

  <p>
    <strong>Pic:</strong>
    <%= post.pic %>
  </p>

</div>
